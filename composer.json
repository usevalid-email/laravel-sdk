{
    "name": "usevalid-email/laravel-sdk",
    "type": "library",
    "description": "Validate Your Emails with Confidence",
    "keywords": [
        "email",
        "validation",
        "sdk",
        "api",
        "usevalid.email"
    ],
    "homepage": "https://usevalid.email",
    "license": "MIT",
    "authors": [
        {
            "name": "Contact",
            "email": "contact@usevalid.email",
            "homepage": "https://usevalid.email"
        }
    ],
    "scripts": {
        "test": "vendor/bin/pest",
        "test:coverage": "vendor/bin/pest --coverage",
        "test:unit": "vendor/bin/pest --testsuite=unit"
    },
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "larastan/larastan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "UseValidEmail\\LaravelSdk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "UseValidEmail\\LaravelSdk\\Tests\\": "tests/"
        },
        "files": [
            "tests/bootstrap.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "UseValidEmail\\LaravelSdk\\Providers\\UseValidEmailServiceProvider"
            ],
            "config": {
                "usevalid-email": "config/usevalid-email.php"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}